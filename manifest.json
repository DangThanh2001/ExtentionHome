{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "Home.html",
        "Home" : "Home.html",
        "default_icon": "/img/icon_ex.jpg"
      },
      "permissions": [
        "tabs"
      ],
      "chrome_url_overrides" : {
        "newtab": "Home.html"
      }
  }